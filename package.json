{
  "name": "uni-shared-utils",
  "version": "1.5.4",
  "description": "uni app shared utils",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "author": "hlhang",
  "license": "ISC",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "keywords": [
    "uniapp",
    "Utils"
  ],
  "scripts": {
    "clean": "node -e \"require('fs').rmSync('dist',{recursive:true,force:true})\"",
    "build:esm": "tsc -p tsconfig.build.esm.json",
    "build:cjs": "tsc -p tsconfig.build.cjs.json",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "postbuild": "node scripts/postbuild.cjs",
    "prepublishOnly": "npm run build",
    "publish": "npm publish"
  },
  "devDependencies": {
    "@dcloudio/types": "3.3.2",
    "@dcloudio/uni-automator": "3.0.0-alpha-3060420220922001",
    "@dcloudio/uni-cli-shared": "3.0.0-alpha-3060420220922001",
    "@dcloudio/uni-stacktracey": "3.0.0-alpha-3060420220922001",
    "@dcloudio/vite-plugin-uni": "3.0.0-alpha-3060420220922001",
    "vite": "^2.9.14",
    "typescript": "^5.6.3",
    "@types/node": "^20"
  },
  "dependencies": {
    "json2typescript": "^1.5.1",
    "ts-dev-common-utils": "1.1.0",
    "tslib": "^2.4.1"
  }
}
